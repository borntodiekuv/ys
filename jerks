_G.Color = Color3.fromRGB(66, 135, 245)
_G.Color2 = Color3.fromRGB(66, 135, 245)

local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/slightten/ui-libs/main/funni"))()
local window = library:CreateWindow("RLT", Enum.KeyCode.RightControl)
local tab1 = window:CreateTab("Main")



local Trainers = {}

for i,v in pairs(game:GetService("Workspace").NPCs.Trainers:GetChildren()) do
    if v:IsA"Model" and v.Parent.Name == "Trainers" then
        table.insert(Trainers,tostring(v))
    end
end

local Trainers = {}

for i,v in pairs(game:GetService("Workspace").NPCs.Trainers:GetChildren()) do
    if v:IsA"Model" and v.Parent.Name == "Trainers" then
        table.insert(Trainers,tostring(v))
    end
end

local Clothing = {}

for i,v in pairs(game:GetService("Workspace").NPCs.Clothing:GetChildren()) do
    if v:IsA"Model" and v.Parent.Name == "Clothing" then
        table.insert(Clothing,tostring(v))
    end
end

local Obelisks = {}

for i,v in pairs(game:GetService("Workspace").NPCs.Obelisks:GetChildren()) do
    if v:IsA"Model" and v.Parent.Name == "Obelisks" then
        table.insert(Obelisks,tostring(v))
    end
end


local Misc = {}

for i,v in pairs(game:GetService("Workspace").NPCs.Misc:GetChildren()) do
    if v:IsA"Model" and v.Parent.Name == "Misc" then
        table.insert(Misc,tostring(v))
    end
end


local sect1 = tab1:CreateSector("NPCs", 'left')



sect1:AddDropdown("Select Trainer",Trainers, 'Select Trainer', false, function(Selected)
getgenv().SelectedNPC = Selected
end)

sect1:AddButton("Button", function() 

    fireclickdetector(game:GetService("Workspace").NPCs.Trainers[SelectedNPC]:FindFirstChild("ClickDetector"))

end)

sect1:AddDropdown("Select Clothing",Clothing, 'Select Clothing', false, function(Selected)
    getgenv().SelectedCloth = Selected
end)
    
    sect1:AddButton("Button", function() 
    
        fireclickdetector(game:GetService("Workspace").NPCs.Clothing[SelectedCloth]:FindFirstChild("ClickDetector"))
end)

sect1:AddDropdown("Select Obelisks",Obelisks, 'Select Obelisks', false, function(Selected)
    getgenv().SelectedObelisks = Selected
end)

    sect1:AddButton("Button", function() 
    
        fireclickdetector(game:GetService("Workspace").NPCs.Clothing[SelectedCloth]:FindFirstChild("ClickDetector"))
end)

sect1:AddDropdown("Select Obelisks",Obelisks, 'Select Obelisks', false, function(Selected)
    getgenv().SelectedObelisks = Selected
end)
    
    sect1:AddButton("Button", function() 
    
        fireclickdetector(game:GetService("Workspace").NPCs.Obelisks[SelectedObelisks]:FindFirstChild("ClickDetector"))

end)


sect1:AddDropdown("Select Misc",Misc, 'Select Misc', false, function(Selected)
    getgenv().SelectedMisc = Selected
end)
    
    sect1:AddButton("Button", function() 
    
        fireclickdetector(game:GetService("Workspace").NPCs.Misc[SelectedMisc]:FindFirstChild("ClickDetector"))

end)

sect1:AddButton("Banker", function() 
    
    fireclickdetector(game:GetService("Workspace").NPCs.Bankers.Banker:FindFirstChild("ClickDetector"))

end)

sect1:AddButton("Engineer", function() 
    
    fireclickdetector(game:GetService("Workspace").NPCs.Engineers.Engineer.ClickPart:FindFirstChild("ClickDetector"))

end)
